<form class="form-horizontal">      	 
	 <div >
		<div class="row-fluid">
		    <div class="span4"><h4>{{ _('Firmware Version:') }}</h4></div>
		</div>
			
		<div class="row-fluid" style="margin-bottom: 5px">
		    <label class="span3" ><h5>{{ _('') }}</h5></label>			    
		     <div class="controls">			    
			<span data-bind="text:fastbot_firmwareVersion" ></span>				
		    </div>
		</div>
	 </div>
    
    
	<div >
		<div class="row-fluid">
		    <div class="span4"><h4>{{ _('Temperature curve ') }}</h4></div>
		</div>
			
		<div class="row-fluid" style="margin-bottom: 5px">
		    <label class="span3" ><h5>{{ _('Extruder1:') }}</h5></label>			    
		     <div class="controls">			    
		     <span class="btn fileinput-button span12" data-bind="css: {disabled: !$root.loginState.isUser()}" style="width: 25%; ">
		        <span>{{ _('Upload') }}</span>
		        <input id="extruder1_curve_upload" type="file" name="file" class="fileinput-button" data-bind="enable: loginState.isUser() && printerState.isOperational()" >
		    </span>			
		    </div>
		</div>
		<div class="row-fluid" style="margin-bottom: 5px">
		    <label class="span3" ><h5>{{ _('Extruder2:') }}</h5></label>			    
		     <div class="controls">			    
			<span class="btn fileinput-button span12" data-bind="css: {disabled: !$root.loginState.isUser()}" style="width: 25%; ">
			    <span>{{ _('Upload') }}</span>
			    <input id="extruder2_curve_upload" type="file" name="file" class="fileinput-button" data-bind="enable: loginState.isUser() && printerState.isOperational()" >
			</span>			    </div>
		</div>
		<div class="row-fluid" style="margin-bottom: 5px">
		    <label class="span3" ><h5>{{ _('Bed:') }}</h5></label>			    
		     <div class="controls">			    
			<span class="btn fileinput-button span12" data-bind="css: {disabled: !$root.loginState.isUser()}" style="width: 25%; ">
			    <span>{{ _('Upload') }}</span>
			    <input id="bed_curve_upload" type="file" name="file" class="fileinput-button" data-bind="enable: loginState.isUser() && printerState.isOperational()" >
			</span>	
		    </div>
		</div>	
	</div>
</form>
